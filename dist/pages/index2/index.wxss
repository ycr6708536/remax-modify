.app {
  text-align: center;
  padding: 0 24rpx;
}

.logo {
  animation: logo-spin infinite 20s linear;
  height: 240rpx;
  width: 240rpx;
  pointer-events: none;
}

.text {
  margin-top: 64rpx;
}

.header {
  padding-top: 30%;
  background-color: #282c34;
  font-size: 40rpx;
  color: white;
}

@keyframes logo-spin {
  from {
    transform: rotate(0deg);
  }
  to {
    transform: rotate(360deg);
  }
}

